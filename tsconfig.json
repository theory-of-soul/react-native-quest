{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "inlineSources": true,
    "noEmit": true,
    "types": [
      "react",
      "react-native",
      "react-redux",
      "immutable",
      "redux-thunk",
      "redux"
    ],
    "baseUrl": "./",
    "paths": {
      "immutable": ["./node_modules/@types/immutable/index.d.ts"],
      "react-native-responsive-screen": ["./files/declarations/react-native-responsive-screen.d.ts"],
    },
    "typeRoots": ["node_modules/@types/"]
  }
}